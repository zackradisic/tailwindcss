      "import": "./dist/index.mjs"
    }
  },
  "dependencies": {
    "@tailwindcss/node": "workspace:^",
    "@tailwindcss/oxide": "workspace:^"
  },
  "devDependencies": {
    "@types/bun": "^1.2.0"
  }
